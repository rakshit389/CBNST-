#include<stdio.h>

float findValueAt(float x)
{
    return 1/x ;
}

int main()
{
    int n;
    float i,a,b,sum=0,h;

    printf("Enter Value of a and b\n");
    scanf("%f%f",&a,&b);
    
    printf("Enter no. of Intervals\n");
    scanf("%d",&n);
 

    h=(b-a)/n ;
    
    sum = findValueAt(a) +findValueAt(b);
    
    int term = 1 ;
    
    for(i=a+h;i<b;i=i+h)
    {
       if( term %2 ==0)
         sum = sum + 2*findValueAt(i);
         
       else
         sum = sum + 4*findValueAt(i); 
        
        term++; 
    }
    
    sum = (h * sum)/3;

    printf("\nValue of The integral  = %f",sum);                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
    
}
